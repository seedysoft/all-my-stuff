{
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "../logs/{ApplicationName}Log-.txt",
          "retainedFileCountLimit": 7,
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
